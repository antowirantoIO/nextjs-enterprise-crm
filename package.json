{
  "name": "nextjs-enterprise-crm",
  "version": "0.1.0",
  "private": true,
  "description": "Enterprise-grade CRM built with Next.js 15, Supabase, and TypeScript",
  "author": "antowirantoIO",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "db:generate": "supabase gen types typescript --project-id=$NEXT_PUBLIC_SUPABASE_PROJECT_ID --schema=public > src/lib/types/database.types.ts",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset",
    "db:seed": "supabase db seed",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "analyze": "cross-env ANALYZE=true next build",
    "postinstall": "husky install",
    "prepare": "husky install",
    "release": "semantic-release",
    "clean": "rm -rf .next out dist",
    "deps:update": "ncu -u && pnpm install",
    "security:audit": "pnpm audit --audit-level moderate"
  },
